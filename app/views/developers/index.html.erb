<h1>Developers</h1>

<table class="table">
  <thead>
    <tr>
      <th>Fullname</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% developers.each do |developer| %>
      <tr>
        <td><%= developer.fullname %></td>
        <td><%= developer.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Developer', new_developer_path, class: "btn btn-primary" %>
